<!DOCTYPE html>
<html lang="en">
<head>
    <meta charset="UTF-8">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Document</title>
</head>
<body>
    <div class="table data" id="flex-container">
        <div class "c1">
            <h2>Mark sheet</h2>
        </div>
        <div class "c1">
            <table border="1">
                <tr>
                    <th>S.no</th>
                    <th>Name</th>
                    <th>Roll No</th>
                    <th>Date</th>
                    <th>Test 1</th>
                    <th>Test 2</th>
                    <th>Test 3</th>
                    <th>Average Score</th>
                </tr>
                <% if(userData.length!=0){%>
                <%var i=1;%>
            <%userData.forEach{function(data){%>
            <tr>
                <td><%=i;%></td>
                <td><%=data.name %></td>
                <td><%=data.reg_no%></td>
                <% var datemonth=data.Date_of_exam.getMonth()+1%>
                <td><%=data.email%></td>
                <td><%=data.t1%></td>
                <td><%=data.t2%></td>
                <td><%=data.t3%></td>
                <td><%=Math.round((data.t1+data.t2+data.t3)/3)%></td>
            </tr>
        <% i++;})%>
        <%} else{%>
        <tr>
            <td colspan="9">No data found</td>
        </tr>
        
        }
        
        
        }}}
            </table>
        </div>
    </div>
</body>
</html>