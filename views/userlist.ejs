<html>
</head>
<body>
    <div class="table" >
        <div class "c1">
            <h2>MARK SHEET</h2>
        </div>
        <div class "c1">
            <table border="4">
                <tr>
                    <th>S.no</th>
                    <th>Name</th>
                    <th>Roll No</th>
                    <th>Date Of Exam</th>
                    <th>Test 1</th>
                    <th>Test 2</th>
                    <th>Test 3</th>
                    <th>Average Score</th>
                </tr>
                <% if(Data.length!=0){%>
                
            <% Data.forEach ((data) => {%>
                <%var i=1;%>
            <tr>
                <td><%= i; %></td>
                <td><%= data.username %></td>
                <td><%= data.reg_no %></td>
                <% var datemonth=data.exam_date.getMonth()+1%>
                <td><%= data.email %></td>
                <td><%= data.t1 %></td>
                <td><%= data.t2 %></td>
                <td><%= data.t3 %></td>
                <td><%= Math.round((data.t1+data.t2+data.t3)/3)%></td>
            </tr>
        <% i++;%>
        <% }); %>
        <% } else{ %>
        <tr>
            <td colspan="9">No data found!!! please check your details..</td>
        </tr>
        
        <% } %>
        
        
        
            </table>
        </div>
    </div>
</body>
</html>